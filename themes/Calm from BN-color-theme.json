{
	"tokenColors": [
		{
			"settings": {
				"background": "#202020",
				"foreground": "#E2E2E5",
				"selectionBorder": "#888",
				"selectionCornerStyle": "square",
				"misspelling": "#6ff",
				"highlight": "#6ff"
			}
		},
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#aa9988"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FAF4C6"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#E2E2E5"
			}
		},
		{
			"name": "Invalid – Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#202020",
				"background": "#777777"
			}
		},
		{
			"name": "Invalid – Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"background": "#777777",
				"foreground": "#202020",
				"fontStyle": "bold"
			}
		},
		{
			"name": "-----------------------------------",
			"settings": {}
		},
		{
			"name": "Keyword",
			"scope": "keyword.declaration",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "-----------------------------------",
			"settings": {}
		},
		{
			"name": "Embedded Source",
			"scope": "text source",
			"settings": {
				"background": "#292929"
			}
		},
		{
			"name": "Entity inherited-class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9B5C2E"
			}
		},
		{
			"name": "String constant",
			"scope": "string constant",
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Regexp «special»",
			"scope": [
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "Variable",
			"scope": "string variable",
			"settings": {
				"foreground": "#E2E2E5"
			}
		},
		{
			"name": "A word 'function' of call",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "? Support.constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#FFFFFF"
			}
		},
		{
			"name": "c C/C++ Preprocessor Line",
			"scope": "meta.preprocessor.c",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "c C/C++ Preprocessor Directive",
			"scope": "meta.preprocessor.c keyword",
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "Doctype/XML Processing",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype entity",
				"meta.tag.sgml.doctype string",
				"meta.tag.preprocessor.xml",
				"meta.tag.preprocessor.xml entity",
				"meta.tag.preprocessor.xml string"
			],
			"settings": {
				"foreground": "#40bf5f"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"declaration.tag",
				"declaration.tag entity",
				"meta.tag",
				"meta.tag entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Inline tags",
			"scope": [
				"declaration.tag.inline",
				"declaration.tag.inline entity",
				"source entity.name.tag",
				"source entity.other.attribute-name",
				"meta.tag.inline",
				"meta.tag.inline entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS tag-name",
			"scope": "meta.selector.css entity.name.tag",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS pseudo-class",
			"scope": "meta.selector.css entity.other.attribute-name.tag.pseudo-class",
			"settings": {
				"foreground": "#E2E2E5"
			}
		},
		{
			"name": "CSS id",
			"scope": "meta.selector.css entity.other.attribute-name.id",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS class",
			"scope": "meta.selector.css entity.other.attribute-name.class",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS property-name:",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#E2E2E5"
			}
		},
		{
			"name": "? CSS property-value;",
			"scope": [
				"meta.property-group support.constant.property-value.css",
				"meta.property-value support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#AA9988"
			}
		},
		{
			"name": "CSS @at-rule",
			"scope": "meta.preprocessor.at-rule keyword.control.at-rule",
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "CSS additional-constants",
			"scope": [
				"meta.property-value support.constant.named-color.css",
				"meta.property-value constant"
			],
			"settings": {
				"foreground": "#FF9800"
			}
		},
		{
			"name": "CSS constructor.argument",
			"scope": "meta.constructor.argument.css",
			"settings": {
				"foreground": "#8F9D6A"
			}
		}
	],
	"colors": {
		"editor.background": "#202020",
		"editorCursor.foreground": "#aaa",
		"editor.foreground": "#E2E2E5",
		"editorWhitespace.foreground": "#555",
		"editor.lineHighlightBackground": "#2d2d2d",
		"editor.selectionBackground": "#393946",
		
		"editor.inactiveSelectionBackground": "#282832",
		
		"editor.selectionHighlightBackground": "#00000000",
		"editor.selectionHighlightBorder": "#ffffff",
		
		"editor.wordHighlightBackground": "#00000000",
		"editor.wordHighlightBorder": "#88BB77",
		
		"editor.wordHighlightStrongBackground": "#00000000",
		"editor.wordHighlightStrongBorder": "#7E8AA2",
		
		"editor.findMatchBackground": "#00000000",
		"editor.findMatchBorder": "#ff5555",
		
		"editor.findMatchHighlightBackground": "#00000000",
		"editor.findMatchHighlightBorder": "#00ff00",
		
		"editor.findRangeHighlightBackground": "#00000080",
		"editor.findRangeHighlightBorder": "#ff000000",
	},
	"name": "Calm from BN"
}