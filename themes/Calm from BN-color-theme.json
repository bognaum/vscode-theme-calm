{
	"tokenColors": [
		{
			"name": "Comment",
			"scope": "comment",
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Constant",
			"scope": "constant",
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "Entity",
			"scope": "entity",
			"settings": {
				"fontStyle": "bold",
				"foreground": "#DDD"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Storage",
			"scope": "storage",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "String",
			"scope": "string",
			"settings": {
				"fontStyle": "",
				"foreground": "#aa9988"
			}
		},
		{
			"name": "Support",
			"scope": "support",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#FAF4C6"
			}
		},
		{
			"name": "Variable",
			"scope": "variable",
			"settings": {
				"foreground": "#D2D2D5"
			}
		},
		{
			"name": "Invalid – Deprecated",
			"scope": "invalid.deprecated",
			"settings": {
				"fontStyle": "bold underline",
				"foreground": "#f59",
			}
		},
		{
			"name": "Invalid – Illegal",
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#f59",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Keyword",
			"scope": "keyword.declaration",
			"settings": {
				"fontStyle": "italic"
			}
		},
		{
			"name": "Entity inherited-class",
			"scope": "entity.other.inherited-class",
			"settings": {
				"fontStyle": "italic",
				"foreground": "#9B5C2E"
			}
		},
		{
			"name": "String constant",
			"scope": "string constant",
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "Regexp",
			"scope": "string.regexp",
			"settings": {
				"fontStyle": "",
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Regexp «special»",
			"scope": [
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "Variable",
			"scope": "string variable",
			"settings": {
				"foreground": "#D2D2D5"
			}
		},
		{
			"name": "A word 'function' of call",
			"scope": "support.function",
			"settings": {
				"fontStyle": "",
				"foreground": "#DDD"
			}
		},
		{
			"name": "? Support.constant",
			"scope": "support.constant",
			"settings": {
				"fontStyle": "",
				"foreground": "#DDD"
			}
		},
		{
			"name": "c C/C++ Preprocessor Line",
			"scope": "meta.preprocessor.c",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "c C/C++ Preprocessor Directive",
			"scope": "meta.preprocessor.c keyword",
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "Doctype/XML Processing",
			"scope": [
				"meta.tag.sgml.doctype",
				"meta.tag.sgml.doctype entity",
				"meta.tag.sgml.doctype string",
				"meta.tag.preprocessor.xml",
				"meta.tag.preprocessor.xml entity",
				"meta.tag.preprocessor.xml string"
			],
			"settings": {
				"foreground": "#40bf5f"
			}
		},
		{
			"name": "Tags",
			"scope": [
				"declaration.tag",
				"declaration.tag entity",
				"meta.tag",
				"meta.tag entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "Inline tags",
			"scope": [
				"declaration.tag.inline",
				"declaration.tag.inline entity",
				"source entity.name.tag",
				"source entity.other.attribute-name",
				"meta.tag.inline",
				"meta.tag.inline entity"
			],
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS tag-name",
			"scope": "meta.selector.css entity.name.tag",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS pseudo-class",
			"scope": "meta.selector.css entity.other.attribute-name.tag.pseudo-class",
			"settings": {
				"foreground": "#D2D2D5"
			}
		},
		{
			"name": "CSS id",
			"scope": "meta.selector.css entity.other.attribute-name.id",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS class",
			"scope": "meta.selector.css entity.other.attribute-name.class",
			"settings": {
				"foreground": "#7E8AA2"
			}
		},
		{
			"name": "CSS property-name:",
			"scope": "support.type.property-name.css",
			"settings": {
				"foreground": "#D2D2D5"
			}
		},
		{
			"name": "? CSS property-value;",
			"scope": [
				"meta.property-group support.constant.property-value.css",
				"meta.property-value support.constant.property-value.css"
			],
			"settings": {
				"foreground": "#AA9988"
			}
		},
		{
			"name": "CSS @at-rule",
			"scope": "meta.preprocessor.at-rule keyword.control.at-rule",
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "CSS additional-constants",
			"scope": [
				"meta.property-value support.constant.named-color.css",
				"meta.property-value constant"
			],
			"settings": {
				"foreground": "#ffcb7d"
			}
		},
		{
			"name": "CSS constructor.argument",
			"scope": "meta.constructor.argument.css",
			"settings": {
				"foreground": "#8F9D6A"
			}
		}
	],
